<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neu.person.mapper.PersonMapper">
    <insert id="addPerson" parameterType="com.neu.person.entity.Person">
        insert into person(id,name,age,address,dept_id,time,path)
        values(#{id},#{name},#{age},#{address},#{dept_id},#{time},#{path})
    </insert>
</mapper>